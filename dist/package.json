{
  "name": "logseq-highlight-importer",
  "version": "0.0.1",
  "description": "Typescript Project starting template",
  "main": "dist/index.html",
  "logseq": {
    "id": "_9h4j38ij8"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && concurrently npm:build:js npm:build:types",
    "build:js": "npm run ts src/build.ts",
    "build:types": "tsc -p tsconfig-build.json --noEmit",
    "build:watch:js": "npm run ts src/build.ts -- --watch",
    "build:watch:types": "tsc -p tsconfig-build.json --noEmit --watch --incremental --preserveWatchOutput",
    "new": "plop --plopfile templates/plopfile.js",
    "new:component": "plop --plopfile templates/plopfile.js component",
    "new:hook": "plop --plopfile templates/plopfile.js hook",
    "new:module": "plop --plopfile templates/plopfile.js module",
    "start": "concurrently npm:build:watch:* npm:test:watch",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "lint": "eslint src/*",
    "ts": "node -r esbuild-register"
  },
  "author": "Kevin LaFlamme",
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@testing-library/dom": "^8.11.3",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.0.3",
    "@types/lodash.debounce": "^4.0.6",
    "@types/minimatch": "^3.0.5",
    "@types/node": "^17.0.4",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/yargs": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "autoprefixer": "^10.4.2",
    "chokidar": "^3.5.3",
    "concurrently": "^7.0.0",
    "cpy": "^9.0.0",
    "esbuild": "^0.14.25",
    "esbuild-register": "^3.3.2",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "globby": "^13.1.0",
    "hnfavs": "^0.0.12",
    "jest": "^27.4.4",
    "lodash.debounce": "^4.0.8",
    "minimatch": "^3.0.4",
    "plop": "^2.7.6",
    "postcss": "^8.4.5",
    "postcss-load-config": "^3.1.1",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.1",
    "typescript": "^4.5.3",
    "xray": "^0.5.1",
    "yargs": "^17.3.1"
  },
  "dependencies": {
    "@logseq/libs": "0.0.1-alpha.35",
    "logseq-dateutils": "0.0.14",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "tailwindcss": "3.0.17",
    "ts-pattern": "3.3.5"
  }
}